Package: blavaan
Title: Bayesian Latent Variable Analysis
Version: 0.1-3.19
Authors@R: c(person(given = "Edgar", family = "Merkle",
                    role = c("aut", "cre"),
                    email = "merklee@missouri.edu"),
             person(given = "Yves", family = "Rosseel",
                    role  = c("aut"),
                    email = "Yves.Rosseel@UGent.be")
